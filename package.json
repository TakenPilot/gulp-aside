{
  "name": "gulp-aside",
  "version": "1.0.1",
  "description": "Apply only to files that match pattern.",
  "main": "index.js",
  "repository": "http://github.com/TakenPilot/gulp-aside.git",
  "scripts": {
    "test": "./node_modules/.bin/mocha -r blanket",
    "coveralls": "./node_modules/.bin/mocha -r blanket -R mocha-lcov-reporter | ./node_modules/coveralls/bin/coveralls.js"
  },
  "config": {
    "blanket": {
      "pattern": "index.js",
      "data-cover-never": "node_modules"
    }
  },
  "keywords": [
    "gulpfriendly",
    "gulpplugin",
    "gulp",
    "filter",
    "aside",
    "through2"
  ],
  "author": "Dane Stuckel",
  "license": "ISC",
  "dependencies": {
    "gulp-util": "^3.0.1",
    "minimatch": "^3.0.4",
    "through2": "^2.0.3"
  },
  "devDependencies": {
    "blanket": "^1.1.6",
    "chai": "^4.1.0",
    "coveralls": "^2.11.2",
    "gulp": "^3.8.10",
    "gulp-concat": "^2.4.1",
    "gulp-rename": "^1.2.0",
    "mocha": "^3.4.2",
    "mocha-lcov-reporter": "^1.3.0",
    "sinon": "^2.4.1",
    "vinyl-buffer": "^1.0.0"
  }
}
